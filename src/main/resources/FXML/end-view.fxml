<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxprojectcsa.Controllers.EndViewController">
   <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0">
      <Label fx:id="endResultLabel" text="You made 10 shots!">
         <font>
            <Font size="60.0" />
         </font>
      </Label>
      <Button fx:id="quitBtn" mnemonicParsing="false" onAction="#onQuitBtnClick" prefHeight="60.0" prefWidth="157.0" text="Quit">
         <font>
            <Font size="35.0" />
         </font>
      </Button>
   </VBox>
</AnchorPane>
